package L4part2;

public class SteeringWheel {
private String type;

public SteeringWheel(String type){
	super();
	this.type=type;
	
}

public String getType() {
	return type;
}

public void setType(String type) {
	this.type = type;
}

@Override
public String toString() {
	return "SteeringWheel [type=" + type + "]";
}

}
