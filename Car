package L4part2;

public class Car {
	private String type;
	private String color;
	
	private Body body;
	private SteeringWheel SteeringWheel;
	private Wheel Wheel;


	public Car(String type, String color) {
		super();
		this.type=type;
		this.color=color;
	}
	public Car(String type, String color,Body body,SteeringWheel SteeringWheel, Wheel Wheel) {
		super();
	this.type=type;
	this.color=color;
	this.body=body;
	this.SteeringWheel=SteeringWheel;
	this.Wheel=Wheel;
}
	public void setType(String type) {
		this.type = type;
	}
	public void setColor(String color) {
		this.color = color;
	}
	public void setBody(Body body) {
		this.body = body;
	}
	public void setSteeringWheel(SteeringWheel steeringWheel) {
		SteeringWheel = steeringWheel;
	}
	public void setWheel(Wheel wheel) {
		Wheel = wheel;
	}
	public String getType() {
		return type;
	}
	public String getColor() {
		return color;
	}
	public Body getBody() {
		return body;
	}
	public SteeringWheel getSteeringWheel() {
		return SteeringWheel;
	}
	public Wheel getWheel() {
		return Wheel;
	}
	@Override
	public String toString() {
		return "Car [type=" + type + ", color=" + color + ", body=" + body + ", SteeringWheel=" + SteeringWheel
				+ ", Wheel=" + Wheel + "]";
	}
	
	

}
